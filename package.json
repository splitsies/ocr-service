{
  "name": "ocr-service",
  "version": "1.0.5",
  "description": "Serverless aws-nodejs-typescript template",
  "main": "serverless.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "serve": "./utils/local-startup/startup.sh",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "clear-shared-models": "npm cache clean --force && rm -rf ./node_modules/@splitsies/shared-models && rm -rf ./package-lock.json",
    "clear-algorithms": "npm cache clean --force && rm -rf ./node_modules/@splitsies/algorithms && rm -rf ./package-lock.json"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "prettier": "@splitsies/formatting",
  "dependencies": {
    "@aws-sdk/client-textract": "^3.465.0",
    "@middy/core": "^3.4.0",
    "@middy/http-json-body-parser": "^3.4.0",
    "@splitsies/formatting": "^1.0.0",
    "@splitsies/shared-models": "^0.0.31",
    "@splitsies/utils": "^0.0.31",
    "firebase-admin": "^11.11.0",
    "image-size": "^1.0.2",
    "inversify": "^6.0.1",
    "reflect-metadata": "^0.1.13",
    "serverless": "^3.27.0",
    "tesseract.js": "^4.0.2",
    "tesseract.js-core": "^4.0.2",
    "@splitsies/algorithms": "^1.0.6"
  },
  "devDependencies": {
    "@serverless/typescript": "^3.0.0",
    "@types/aws-lambda": "^8.10.71",
    "@types/node": "^14.14.25",
    "esbuild": "^0.14.11",
    "json-schema-to-ts": "^1.5.0",
    "prettier": "^2.8.8",
    "serverless-esbuild": "^1.23.3",
    "serverless-offline": "^12.0.4",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT"
}
